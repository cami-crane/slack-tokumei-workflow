{
  "name": "bolt-js-custom-function-template",
  "version": "1.0.0",
  "description": "A custom function template for Slack apps",
  "main": "lib/app.js",
  "scripts": {
    "start": "node app.js",
    "mode:dev": "cp .env.dev .env",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "build:clear": "tsc --clean",
    "lint": "eslint --ext .js .",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 400 --fix",
    "format": "prettier --write './**/*.{js,ts,tsx,html,css,json,md}'"
  },
  "author": "Slack Technologies, LLC",
  "license": "MIT",
  "keywords": [
    "slack",
    "bolt",
    "slackapi"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/slack-samples/bolt-js-custom-function-template.git"
  },
  "bugs": {
    "url": "https://github.com/slack-samples/bolt-js-custom-function-template/issues"
  },
  "dependencies": {
    "@slack/bolt": "3.17.1-customFunctionBeta.0",
    "dotenv": "~16.4.5"
  },
  "devDependencies": {
    "@slack/cli-hooks": "^1.1.0",
    "@types/node": "^20.12.7",
    "eslint": "~8.57.0",
    "eslint-config-airbnb-base": "~15.0.0",
    "eslint-plugin-import": "~2.29.1",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
